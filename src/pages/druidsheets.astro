---
import Layout from '../layouts/IframeLayout.astro';
---
<Layout
	title="Druid Sheet"
	description="Druid Sheet"
	pubDate={new Date('August 08 2021')}
>
  <style>
    .iframe-container {
      width: 100%;
      height: 100vh;
      margin: 0;
      padding: 0;
    }
    .iframe-container iframe {
      width: 100%;
      height: 100%;
      border: none;
    }
  </style>
  
  <div class="iframe-container">
    <iframe src="https://dnddruidsheet.robertmaiorana.workers.dev"></iframe>
  </div>
</Layout>
